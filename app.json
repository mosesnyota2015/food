{
  "expo": {
    "name": "QuickServe Restaurant",
    "slug": "quickserve-restaurant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A2511"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4A2511"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A Nigerian cuisine restaurant app prototype featuring a splash screen, menu of Nigerian meals, and a form for adding new dishes."
  }
}